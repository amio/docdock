{
  "name": "DocDock",
  "short_name": "docdock",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "App launcher for devdocs.io.",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "webview",
    "storage",
    "notifications"
  ],
  "icons": {
    "16": "images/icon-128.png",
    "128": "images/icon-128.png",
    "256": "images/icon-256.png",
    "512": "images/icon-512.png"
  }
}
